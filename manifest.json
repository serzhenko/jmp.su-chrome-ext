{
	"name": "jmp.su URL Shortener",
	"version": "0.1",
	"description": "Shortens URLs via jmp.su URL shortener",
	"background_page": "background.html",
	"icons": {
		"48": "icons/48icon.png",
		"128": "icons/128icon.png"
	},
	"page_action": {
		"default_icon": "icons/icon.png",
		"popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"http://jmp.su/*"
	]
}
